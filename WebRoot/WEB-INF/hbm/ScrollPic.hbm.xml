<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class name="com.web.entity.ScrollPic" table="tbl_scroll_pic">

<id name="id" column="id" type="integer">
<generator class="increment"></generator>
</id>

<property name="name" column="name" type="string"></property>

<property name="urls" column="urls" type="string"></property>
<property name="info" column="info" type="string"></property>
<property name="effective" column="effective" type="boolean"></property>

</class>


</hibernate-mapping>
